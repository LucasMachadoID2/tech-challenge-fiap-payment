// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Payment {
  id         String   @id
  amount     Float
  status     String
  payerId    String
  payerEmail String
  createdAt  DateTime
  updatedAt  DateTime
}
